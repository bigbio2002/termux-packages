+static __inline__ int getloadavg(double __averages[_Nonnull], int __n) {
+  if (__n < 0) return -1;
+  if (__n > 3) __n = 3;
+
+  struct sysinfo si;
+  if (sysinfo(&si) == -1) return -1;
+
+  for (int i = 0; i < __n; ++i) {
+    __averages[i] = (double)si.loads[i] / (double)(1 << SI_LOAD_SHIFT);
+  }
+  return __n;
+}
 
 
